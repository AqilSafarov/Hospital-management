@model VmRegistor
@{
    Layout = null;
}
<!DOCTYPE html>
<html lang="en">

<!-- Mirrored from emr.dreamguystech.com/html/register.html by HTTrack Website Copier/3.x [XR&CO'2014], Fri, 27 Aug 2021 09:15:53 GMT -->
<head>
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0, user-scalable=0">
    <title>EMR - Dashboard</title>

    <link rel="shortcut icon" href="~/areas/admin/assets/img/favicon.png">

    <link rel="stylesheet" href="~/areas/admin/assets/css/bootstrap.min.css">

    <link rel="stylesheet" href="~/areas/admin/assets/plugins/fontawesome/css/fontawesome.min.css">
    <link rel="stylesheet" href="~/areas/admin/assets/plugins/fontawesome/css/all.min.css">

    <link rel="stylesheet" href="~/areas/admin/assets/css/style.css">
</head>
<body>

    <div class="main-wrapper login-body">
        <div class="login-wrapper">
            <div class="container">
                <img class="img-fluid logo-dark mb-2" style="background:white;" src="image/footer-logo.png" alt="Logo">

                <div class="loginbox">
                    <div class="login-right">
                        <div class="login-right-wrap">
                            <h1>Register</h1>
                            <p class="account-subtitle">Access to our dashboard</p>

                            <form method="post">
                                <div asp-validation-summary="All" style="color:red;"></div>

                                <div class="form-group">
                                    <label class="form-control-label">Name</label>
                                    <input class="form-control" asp-for="Name">
                                </div>
                                <div class="form-group">
                                    <label class="form-control-label">Email Address</label>
                                    <input class="form-control" asp-for="Email">
                                </div>
                                <div class="form-group">
                                    <label class="form-control-label">Password</label>
                                    <input class="form-control" asp-for="Password">
                                </div>
                                <div class="form-group">
                                    <label class="form-control-label">Confirm Password</label>
                                    <input class="form-control" asp-for="ConfirmPassword">
                                </div>
                                <div class="form-group mb-0">
                                    <button class="btn btn-lg btn-block btn-primary" type="submit">Registor</button>

                                </div>
                            </form>

                            <div class="login-or">
                                <span class="or-line"></span>
                                <span class="span-or">or</span>
                            </div>

                            <div class="social-login">
                                <span>Register with</span>
                                <a href="#" class="facebook"><i class="fab fa-facebook-f"></i></a><a href="#" class="google"><i class="fab fa-google"></i></a>
                            </div>

                            <div class="text-center dont-have">Already have an account? <a asp-area="Admin" asp-controller="Account" asp-action="Login">Login</a></div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>


    <script src="~/areas/admin/assets/js/jquery-3.6.0.min.js"></script>

    <script src="~/areas/admin/assets/js/bootstrap.bundle.min.js"></script>

    <script src="~/areas/admin/assets/js/script.js"></script>
</body>

<!-- Mirrored from emr.dreamguystech.com/html/register.html by HTTrack Website Copier/3.x [XR&CO'2014], Fri, 27 Aug 2021 09:15:53 GMT -->
</html>
